#!/bin/sh

/usr/bin/dash $HOME/.scripts/work-tmux &
qutebrowser &
vscodium &
/usr/bin/dash $HOME/eureciclo/conectar-bancos/redeciclo-corp-conectar-no-login.sh &
/usr/bin/pritunl-client-electron &
# /usr/bin/dash $HOME/.scripts/pull-repositories.sh &
rm $HOME/Downloads &
/usr/bin/dash $HOME/.scripts/monitoring-tmux &
/usr/bin/dash $HOME/.scripts/show_keybinds.sh &
ferdium &
dbeaver &
# portmaster &
clockify &
/usr/bin/bash $HOME/.scripts/configure-trackball.sh &

bluetoothctl connect CC:98:8B:C1:5C:78; kill -46 $(pidof dwmblocks)

WIFI=`iwctl station wlan0 show | grep -c 'QueWiFi2'` &

if [ $WIFI -ge 1 ] ; then
	/usr/bin/dash $HOME/.scripts/windows-server
else
	virt-manager --connect=qemu:///system --show-domain-console win10
fi
